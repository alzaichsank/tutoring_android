        android:theme="@style/Theme.Tutor_android">
        <activity
            android:name=".tutordesign.intentlearning.IntentSecondsActivity"
            android:exported="false" />
        <activity
            android:name=".tutordesign.intentlearning.IntentFirstActivity"
